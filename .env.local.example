# ==============================================
# FORMBRICKS CONFIGURATION
# ==============================================
# Get these from your Formbricks Cloud project:
# https://app.formbricks.com/

# Environment ID from your Formbricks project settings
NEXT_PUBLIC_FORMBRICKS_ENV_ID=UPDATE_THIS_WITH_YOUR_FORMBRICKS_ENV_ID
# API Host (use https://app.formbricks.com for cloud)
NEXT_PUBLIC_FORMBRICKS_API_HOST=https://app.formbricks.com

# Optional: Server-side API key (if using webhooks)
# FORMBRICKS_API_KEY=UPDATE_THIS_WITH_YOUR_FORMBRICKS_API_KEY
# FORMBRICKS_WEBHOOK_SECRET=UPDATE_THIS_WITH_YOUR_FORMBRICKS_WEBHOOK_SECRET

# Debug mode (true for development, false for production)
FORMBRICKS_DEBUG=true

# ==============================================
# APPLICATION CONFIGURATION
# ==============================================
# Env
NEXT_PUBLIC_SITE_URL=http://localhost:3000
